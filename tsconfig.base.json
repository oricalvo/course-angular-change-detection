 {
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "types": ["node"],
    "declaration": true,
    "declarationMap": true,
    "emitDecoratorMetadata":true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "noEmitOnError": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true
  },
  "exclude": ["node_modules", "dist"]
}
